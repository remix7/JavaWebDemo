<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5"

    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/j2ee/web-app_2.5.xsd">
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>MyServletDemo1</servlet-name>
    <servlet-class>com.test.http.MyServletDemo1</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>MyServletDemo2</servlet-name>
    <servlet-class>com.test.http.MyServletDemo2</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>MyServletDemo3</servlet-name>
    <servlet-class>com.test.http.MyServletDemo3</servlet-class>
  </servlet>



  <servlet-mapping>
    <servlet-name>MyServletDemo1</servlet-name>
    <url-pattern>/servlet/MyServletDemo1</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>MyServletDemo2</servlet-name>
    <url-pattern>/servlet/MyServletDemo2</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>MyServletDemo3</servlet-name>
    <url-pattern>/servlet/MyServletDemo3</url-pattern>
  </servlet-mapping>



</web-app>